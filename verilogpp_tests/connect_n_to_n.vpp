module connect_to_array();

/**AUTONET --init **/
/*PPSTART*/
wire  bar;
/*PPSTOP*/

/**FORINST drive_one.v drive_one 0 1
  .foo(bar[${enumerator}])
**/
/*PPSTART*/
drive_one drive_one_0 (
  .foo (bar[0])
);
drive_one drive_one_1 (
  .foo (bar[1])
);
/*PPSTOP*/

for (genvar ii = 0; ii < 2; ii += 1) begin : g_sink
/**INST sink_one.v sink_one
  .foo(bar[ii])
**/
/*PPSTART*/
sink_one sink_one (
  .foo (bar[ii])
);
/*PPSTOP*/

/**AUTONC**/
/*PPSTART*/

/*PPSTOP*/


endmodule : connect_to_array
